<%= link_to insta_user_post_path(post.insta_user, post) do %>
  <article class='border border-slate-300 hover:border-slate-400 grayscale-0 hover:grayscale'>
    <%= image_tag post.bg_image_url, alt: "Post by #{post.insta_user.username} on #{post.timestamp}", class: 'md:h-64 h-48 w-full object-cover' %>
  </article>
<% end %>
